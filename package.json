    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "jest": "^29.5.0",
    "normalize.css": "^8.0.1",
    "postcss": "^8.4.23",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "postcss-loader": "^7.3.1",
    "precss": "^4.0.0",
    "prettier": "^2.8.8",
